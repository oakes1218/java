package Ninenine;


public class Multiplicationtable {
	public static void main (String [] gera){
		int count = 0;
		
		for (int i = 1 ; i <= 9 ; i++){
			
			for (int j = 1 ; j <= 9 ; j++){	
				
				System.out.println ( i + " X " + j + " = " + i * j + "   ");	
				if(i < 3){
					System.out.println ();
				}

			}
		}	
	}
}
