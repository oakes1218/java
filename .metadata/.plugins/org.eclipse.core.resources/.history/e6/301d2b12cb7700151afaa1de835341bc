package Grade;

import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;
import java.io.ObjectOutputStream.PutField;
import java.security.PublicKey;
import java.util.ArrayList;
import java.util.Collections;
import java.util.HashMap;
import java.util.Map;

public class Readfile {
	
	ArrayList<Object> list_Data = new ArrayList<Object>();
	HashMap<String, Integer> map = new HashMap<String, Integer>();
	String line ;
	
	public void Read() throws IOException {
		
		FileReader fr = new FileReader("/Users/eddie/Downloads/grade1.txt");
		BufferedReader br = new BufferedReader(fr);
		
		while ((line = br.readLine()) != null){
			Student student = new Student();
			String[] d = line.split(",");
			student.setId(Integer.valueOf(d[0]));
			student.setName(d[1]);
			student.setScore(Integer.valueOf(d[2]));
			list_Data.add(student);
			//map.put(student.getName(),student.getScore());
			
		}
		System.out.println(list_Data);
		fr.close();
		//return map;
	}
	

}	
