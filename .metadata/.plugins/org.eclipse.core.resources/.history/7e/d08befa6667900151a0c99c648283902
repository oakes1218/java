package Ninenine;

import java.util.concurrent.CountDownLatch;

public class Multiplicationtable {
	public static void main (String [] gera){
		int count = 0;
		
		for (int i = 1 ; i <= 3 ; i++){
			for (int j = 1 ; j <= 9 ; j++){	
				System.out.print ( i + " X " + j + " = " + i * j + "   ");	
				if (i%9 == 0){
					System.out.println ();
				}
			}
			
		}
		

	}
}
