package Ninenine;

import java.awt.List;
import java.util.ArrayList;


public class Multiplicationtable {
	public  static void main (String [] agre) {
		
		for (int i = 1; i <= 9; i++)
		{
			for (int j = 1; j <= 9; j++)
			{
				int x = ((j - 1) % 3) + 1 + 3 * ((i -1) / 3) ;
				int y = ((j -1) / 3) + 3 * ((i - 1) % 3) + 1;
				System.out.print( x + " x " + y + " = " + (x*y) + "\t");
				if (j != 0 && j % 4 == 0)
				{
					System.out.println();
				}
			}
			if (i != 0 && i % 3 == 0)
			{
				System.out.println();
			}
		}
	}
}
