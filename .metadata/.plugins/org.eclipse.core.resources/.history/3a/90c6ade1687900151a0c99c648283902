package Ninenine;

import java.util.concurrent.CountDownLatch;

public class Multiplicationtable {
	public static void main (String [] gera){
		int count = 0;
		
		for (int i = 1 ; i <= 9 ; i++){
			//count ++;
			for (int j = 1 ; j <= 9 ; j++){	
				//count ++;
				if(count%9 == 1){
				System.out.println ( i + " X " + j + " = " + i * j + "   ");	
				}else {
					continue;
				}

			}
		}	
	}
}
