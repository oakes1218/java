package Ninenine;

import java.awt.List;
import java.util.ArrayList;


public class Multiplicationtable {
	public  static void main (String [] agre) {
		
	for (int k = 1 ; k <= 9 ; k += 3){
		for (int i = 1; i <= 9; i++)
		{
			for (int j = 1; j <= 3; j++)
			{
				System.out.print( j + " x " + i + " = " + (i*j) + "\t");
			}
			System.out.println();
		}
		System.out.println();
		}
	}
}
